{
  "extends": "./react-library.json",
  "Display": "Vite",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "types": ["vite/client", "cypress"],
    "allowJs": false,
    "esModuleInterop": false,
    "module": "ESNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["cypress.d.ts"]
}
