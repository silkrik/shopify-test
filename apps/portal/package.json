{
  "name": "@d5/portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode mock",
    "start:dev": "vite --mode localdev",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode prod",
    "dev": "vite --mode dev",
    "test": "cypress open",
    "build": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --emptyOutDir",
    "build:dev": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode dev --emptyOutDir",
    "build:staging": "tsc && node --max_old_space_size=8192 ../../node_modules/vite/bin/vite.js build --mode staging --emptyOutDir",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bigcommerce/big-design": "^0.35.0",
    "@bigcommerce/big-design-icons": "^0.22.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@shopify/polaris": "^11.13.0",
    "@types/mocha": "^10.0.1",
    "dayjs": "^1.11.7",
    "i18next": "^22.0.6",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@d5/tsconfig": "*",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "styled-components": "^5.3.5",
    "terser": "^5.14.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
